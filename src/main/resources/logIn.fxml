<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2a295c;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LogIn">
   <children>
      <PasswordField fx:id="txtPass" layoutX="300.0" layoutY="217.0" prefHeight="40.0" prefWidth="272.0" promptText="Contraseña" style="-fx-background-radius: 0px;" />
      <Text fill="WHITE" layoutX="323.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Iniciar sesión">
         <font>
            <Font name="Segoe UI Bold" size="35.0" />
         </font></Text>
      <Button fx:id="btnAceptar" ellipsisString="" layoutX="451.0" layoutY="315.0" mnemonicParsing="false" onAction="#btnAceptarActionPerformed" prefHeight="47.0" prefWidth="121.0" style="-fx-background-color: white; -fx-background-radius: 0px;" text="Aceptar" textFill="#2a295c">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Segoe UI Bold" size="15.0" />
         </font>
         <effect>
            <InnerShadow blurType="ONE_PASS_BOX" height="21.26" radius="10.005" width="20.76">
               <color>
                  <Color red="0.16529999673366547" green="0.16200000047683716" blue="0.36000001430511475" opacity="0.591304361820221" />
               </color>
            </InnerShadow>
         </effect></Button>
      <ImageView fitHeight="34.0" fitWidth="27.0" layoutX="266.0" layoutY="174.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/user_white.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="34.0" fitWidth="27.0" layoutX="266.0" layoutY="224.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/password_white.png" />
         </image></ImageView>
      <AnchorPane prefHeight="400.0" prefWidth="250.0" style="-fx-background-color: white;">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="25.0" layoutY="166.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/sodexo.png" />
               </image>
            </ImageView>
         </children></AnchorPane>
      <ComboBox layoutX="300.0" layoutY="167.0" prefHeight="40.0" prefWidth="272.0" promptText="Restaurante" style="-fx-background-color: white; -fx-background-radius: 0px;" />
      <AnchorPane fx:id="panelUserPassIncorrecto" layoutX="108.0" layoutY="107.0" prefHeight="200.0" prefWidth="355.0" style="-fx-background-color: white;" visible="false">
         <effect>
            <DropShadow height="20.0" offsetX="5.0" offsetY="5.0" radius="9.5" width="20.0" />
         </effect>
         <children>
            <Label layoutX="128.0" layoutY="67.0" prefHeight="67.0" prefWidth="200.0" text="Usuario o contraseña incorrectos">
               <font>
                  <Font name="Segoe UI Bold" size="12.0" />
               </font>
            </Label>
            <Button layoutX="242.0" layoutY="146.0" mnemonicParsing="false" onMouseClicked="#btnAceptarUserPassIncorrectoActionPerformed" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: white; -fx-background-radius: 0px;" text="Aceptar">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <effect>
                  <InnerShadow blurType="ONE_PASS_BOX" height="11.87" radius="5.435" width="11.87" />
               </effect>
            </Button>
            <ImageView fitHeight="27.0" fitWidth="27.0" layoutX="328.0" layoutY="1.0" onMouseClicked="#btnAceptarUserPassIncorrectoActionPerformed" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/x.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
            <ImageView fitHeight="78.0" fitWidth="71.0" layoutX="42.0" layoutY="65.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/delete.png" />
               </image></ImageView>
         </children>
      </AnchorPane>
      <Label layoutX="300.0" layoutY="265.0" onMouseClicked="#contrasenaOlvidadaActionPerformed" text="¿Olvidó su contraseña?" textFill="WHITE" underline="true">
         <font>
            <Font name="Segoe UI Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
   </children>
</AnchorPane>
